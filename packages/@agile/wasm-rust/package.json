{
  "name": "@agile/wasm-rust",
  "version": "0.0.1",
  "description": "Rust WASM runtime integration for high-performance computing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:wasm": "cargo build --manifest-path ../../../wasm-modules/rust-core-wasm/Cargo.toml --target wasm32-unknown-unknown --release",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:bench": "vitest bench",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@agile/core": "workspace:*",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@vitest/coverage-v8": "^1.1.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "files": [
    "dist",
    "src",
    "wasm"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}