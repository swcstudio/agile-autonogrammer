{
  "name": "@agile/ai-agents",
  "version": "0.1.0",
  "description": "Multi-modal AI agents system with autonomous task execution and inter-agent collaboration",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./workers": {
      "import": "./dist/workers/index.js",
      "types": "./dist/workers/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agents",
    "multimodal",
    "autonomous",
    "collaboration",
    "task-execution",
    "orchestration",
    "planning"
  ],
  "author": "Agile Programmers",
  "license": "MIT",
  "dependencies": {
    "@agile/ai-core": "workspace:*",
    "@agile/ai-edge": "workspace:*",
    "@agile/ai-security": "workspace:*",
    "@agile/ai-wasm": "workspace:*",
    "@agile/execution-engine": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "uuid": "^9.0.1",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4",
    "lodash-es": "^4.17.21",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}