{
  "name": "@katalyst/core",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts",
    "./components": "./src/components/mod.ts",
    "./hooks": "./src/hooks/mod.ts",
    "./stores": "./src/stores/mod.ts",
    "./utils": "./src/utils/mod.ts",
    "./providers": "./src/providers/mod.ts",
    "./integrations": "./src/integrations/mod.ts"
  },
  "imports": {
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "@tanstack/": "npm:/@tanstack/",
    "@trpc/": "npm:/@trpc/",
    "zustand": "npm:zustand@^4.0.0",
    "zod": "npm:zod@^3.0.0"
  },
  "tasks": {
    "build": "deno run --allow-all build.ts",
    "dev": "deno run --allow-all --watch dev.ts",
    "test": "deno test --allow-all",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["dom", "dom.iterable", "esnext"]
  }
}