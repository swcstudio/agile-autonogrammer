{
  "name": "@katalyst/framework",
  "version": "1.0.0",
  "description": "Production-ready multi-runtime framework with WebAssembly acceleration, AI integration, and unified development experience",
  "keywords": [
    "framework",
    "webassembly",
    "wasm",
    "rust",
    "elixir",
    "typescript",
    "ai",
    "multithreading",
    "performance",
    "deno",
    "react",
    "nextjs"
  ],
  "author": "Katalyst Team",
  "license": "MIT",
  "homepage": "https://katalyst.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/katalyst/framework.git"
  },
  "bugs": {
    "url": "https://github.com/katalyst/framework/issues"
  },
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./wasm": {
      "types": "./dist/wasm/index.d.ts",
      "import": "./dist/wasm/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    },
    "./payments": {
      "types": "./dist/payments/index.d.ts",
      "import": "./dist/payments/index.js"
    },
    "./design-system": {
      "types": "./dist/design-system/index.d.ts",
      "import": "./dist/design-system/index.js"
    },
    "./components": "./dist/components/index.js",
    "./hooks": "./dist/hooks/index.js",
    "./stores": "./dist/stores/index.js",
    "./providers": "./dist/providers/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "wasm/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts && npm run build:bundle",
    "build:wasm": "cd ../wasm-runtimes && ./build-all.sh && cp -r ../../public/wasm ./wasm/",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:bundle": "rollup -c rollup.config.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist/ && rm -rf wasm/",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@tanstack/react-query": ">=5.0.0",
    "@tanstack/react-router": ">=1.0.0",
    "@trpc/client": ">=10.0.0",
    "zustand": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "@tanstack/react-router": {
      "optional": true
    },
    "@trpc/client": {
      "optional": true
    },
    "zustand": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}